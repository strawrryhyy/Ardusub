d={'Michael' :95, 'Bob':75, 'Tracy':85}

print(d['Bob'])

d['Tracy']=66
print(d['Tracy'])

print('Tomas' in d)

print(d.get('Tomas',-1))

d.pop('Bob')
print(d)

